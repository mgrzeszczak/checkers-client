<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<VBox fx:controller="pl.mg.checkers.controller.LobbyController" maxHeight="-Infinity" maxWidth="-Infinity"
      minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
<children>
    <HBox alignment="CENTER" spacing="20.0" VBox.vgrow="NEVER">
        <children>
            <Label text="Lobbies">
                <font>
                    <Font size="20.0" />
                </font>
            </Label>
            <Button mnemonicParsing="false" text="Create lobby" fx:id="createLobbyButton" onAction="#onCreateLobbyButtonClick" />
            <Button mnemonicParsing="false" fx:id="refreshButton" text="Refresh" onAction="#onRefreshButtonClick" />
            <Button mnemonicParsing="false" fx:id="logoutButton" text="Log out" onAction="#onLogoutButtonClick" />
        </children>
        <padding>
            <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
        </padding>
    </HBox>
    <AnchorPane prefHeight="200.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
        <children>
            <ListView fx:id="listView" layoutX="200.0" layoutY="68.0" prefHeight="200.0" prefWidth="200.0"
                      AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
        </children>
    </AnchorPane>
</children>
</VBox>